<plugin name="radar" url="%SWFPATH%/plugins/radar.swf" alturl="%SWFPATH%/plugins/radar.js" zorder="1006" keep="true" heading="0" parent="map-view-radar" align="lefttop" edge="center" x="0" y="0" linecolor="0" fillcolor="0xFBB438" alpha="1" scale="0.6" visible="false" editmode="true"/>
<action name="activatespot">
set(plugin[activespot].parent, plugin[%1]);
set(plugin[activespot].visible, true);
copy(plugin[radar].x, plugin[%1].x);
copy(plugin[radar].y, plugin[%1].y);
set(plugin[radar].visible, true);
set(plugin[radar].alpha,1);
set(plugin[radar].heading, %2);
</action>
 
<action name="update_map_spot">
activatespot(%1,%2);
loadscene(%1, null, MERGE, BLEND(1));
</action>
<action name="activatespotRadar">
activatespot(%1);
</action>
